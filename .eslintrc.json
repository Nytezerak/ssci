{
  "rules": {
    "react/react-in-jsx-scope": 0, 
    "import/prefer-default-export": 0
  }, 
  "extends": [
    "plugin:react/recommended", 
    "airbnb"
  ], 
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }, 
    "sourceType": "module", 
    "ecmaVersion": 12
  }, 
  "env": {
    "es2021": true, 
    "browser": true
  }, 
  "plugins": [
    "react"
  ]
}